#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Project/hackaton/gogobit/gogobit-backend/doma-graph/gogobidoma-subgraph/node_modules/.pnpm/@graphprotocol+graph-cli@0._91d4e26192fec040dd71aceea1c1484e/node_modules/@graphprotocol/graph-cli/bin/node_modules:/mnt/c/Project/hackaton/gogobit/gogobit-backend/doma-graph/gogobidoma-subgraph/node_modules/.pnpm/@graphprotocol+graph-cli@0._91d4e26192fec040dd71aceea1c1484e/node_modules/@graphprotocol/graph-cli/node_modules:/mnt/c/Project/hackaton/gogobit/gogobit-backend/doma-graph/gogobidoma-subgraph/node_modules/.pnpm/@graphprotocol+graph-cli@0._91d4e26192fec040dd71aceea1c1484e/node_modules/@graphprotocol/node_modules:/mnt/c/Project/hackaton/gogobit/gogobit-backend/doma-graph/gogobidoma-subgraph/node_modules/.pnpm/@graphprotocol+graph-cli@0._91d4e26192fec040dd71aceea1c1484e/node_modules:/mnt/c/Project/hackaton/gogobit/gogobit-backend/doma-graph/gogobidoma-subgraph/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Project/hackaton/gogobit/gogobit-backend/doma-graph/gogobidoma-subgraph/node_modules/.pnpm/@graphprotocol+graph-cli@0._91d4e26192fec040dd71aceea1c1484e/node_modules/@graphprotocol/graph-cli/bin/node_modules:/mnt/c/Project/hackaton/gogobit/gogobit-backend/doma-graph/gogobidoma-subgraph/node_modules/.pnpm/@graphprotocol+graph-cli@0._91d4e26192fec040dd71aceea1c1484e/node_modules/@graphprotocol/graph-cli/node_modules:/mnt/c/Project/hackaton/gogobit/gogobit-backend/doma-graph/gogobidoma-subgraph/node_modules/.pnpm/@graphprotocol+graph-cli@0._91d4e26192fec040dd71aceea1c1484e/node_modules/@graphprotocol/node_modules:/mnt/c/Project/hackaton/gogobit/gogobit-backend/doma-graph/gogobidoma-subgraph/node_modules/.pnpm/@graphprotocol+graph-cli@0._91d4e26192fec040dd71aceea1c1484e/node_modules:/mnt/c/Project/hackaton/gogobit/gogobit-backend/doma-graph/gogobidoma-subgraph/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/run" "$@"
else
  exec node  "$basedir/../../bin/run" "$@"
fi
